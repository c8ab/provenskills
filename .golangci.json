{
  "formatters": {
    "exclusions": {
      "generated": "lax",
      "paths": [
        "third_party$",
        "builtin$",
        "examples$"
      ]
    }
  },
  "linters": {
    "enable": [
      "gocritic"
    ],
    "exclusions": {
      "generated": "lax",
      "paths": [
        "third_party$",
        "builtin$",
        "examples$"
      ],
      "presets": [
        "comments",
        "common-false-positives",
        "legacy",
        "std-error-handling"
      ]
    },
    "settings": {
      "gocritic": {
        "enabled-tags": [
          "diagnostic",
          "style"
        ]
      }
    }
  },
  "version": "2"
}
